.py-pagenation-container {
  height: 28px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.py-pagenation-btn {
  width: 28px;
  height: 28px;
  margin-left: 4px;
}

.py-pagenation-btn.active {
  background-color: #FFF9F6;
  border: 1px solid #F68468;
  color: #F68468;
}

.py-pagenation-label {
  font-size: 12px;
  color: #3F3F3F;
  width: 35px;
  text-align: right;
}